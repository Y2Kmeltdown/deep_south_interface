--synthesis_resources = reg 4 
OPTIONS ALTERA_INTERNAL_OPTION = "AUTO_SHIFT_REGISTER_RECOGNITION=OFF";

SUBDESIGN dffpipe_k5c
( 
	clock	:	input;
	clrn	:	input;
	d[3..0]	:	input;
	q[3..0]	:	output;
) 
VARIABLE 
	dffe3a[3..0] : dffe;
	ena	: NODE;
	prn	: NODE;
	sclr	: NODE;

BEGIN 
	dffe3a[].clk = clock;
	dffe3a[].clrn = clrn;
	dffe3a[].d = (d[] & (! sclr));
	dffe3a[].ena = ena;
	dffe3a[].prn = prn;
	ena = VCC;
	prn = VCC;
	q[] = dffe3a[].q;
	sclr = GND;
END;
--VALID FILE
